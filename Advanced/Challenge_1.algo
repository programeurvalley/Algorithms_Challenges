ALGORITHM Challenge_Advanced_1
VAR
    x , y , z : INTEGER;
    i : INTEGER;
BEGIN
    Write("Entre the value of x :");
    Read(x);
    Write("Entre the value of y :");
    Read(y);
    Write("Entre the value of z :");
    Read(z);
    
    i := x ;
    x := y; 
    y := z ;
    z := i ;
    
    Write("After permutations we have x :", x , " and y :" , y , "and z : " , z );
    
END

